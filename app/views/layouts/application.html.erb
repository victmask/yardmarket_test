<!DOCTYPE html>
<html>
  <head>
    <title>Yardmarket | <%= yield(:title) %></title>
    <%= javascript_include_tag :defaults %>
    <%= stylesheet_link_tag :all %>
    <%= render 'layouts/stylesheets' %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div class="container">
      <%= render 'layouts/header' %>
      <section class="round">
        <%= yield %>
      </section>
      <%= render 'layouts/footer' %>

    </div>

  </body>
</html>